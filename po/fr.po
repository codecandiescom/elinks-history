msgstr "Couleurs de la barre de menu."

#: src/config/options.c:1927 src/config/options.c:2322
msgid "Unselected menu item"
msgstr "Item non sélectionné du menu"

#: src/config/options.c:1929 src/config/options.c:2324
msgid "Unselected menu item colors."
msgstr "Couleurs des items non sélectionnés du menu."

#: src/config/options.c:1939 src/config/options.c:2334
msgid "Selected menu item"
msgstr "Item sélectionné du menu"

#: src/config/options.c:1941 src/config/options.c:2336
