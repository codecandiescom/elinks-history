#: src/config/options.inc:551
msgid "Allow dark colors on black background"
msgstr "Fekete háttéren mutassa a sötét színeket"

#: src/config/options.inc:553
msgid "Allow dark colors on black background."
msgstr "Fekete háttéren mutassa a sötét színeket."

