msgstr "Afficher subscripts (comme [chose])."

#: src/config/options.c:1504
msgid "Display superscripts"
msgstr "Afficher superscripts"

#: src/config/options.c:1506
msgid "Display superscripts (as ^thing)."
msgstr "Afficher superscripts (comme ^chose)."

#: src/config/options.c:1508
msgid "Rendering of html link element"
msgstr "Rendu des éléments html link"

#: src/config/options.c:1510
msgid ""
"Html link rendering verbosity:\n"
"0 is nothing\n"
"1 is title\n"
"2 is name in addition\n"
"3 is hreflang in addition\n"
"4 is type in addition\n"
"5 is everything"
msgstr ""
"Rendu des éléments html link:\n"
"0 rien\n"
"1 titre\n"
"2 nom en plus\n"
"3 langue en plus (hreflang)\n"
"4 type de contenu en plus\n"
"5 tout"

